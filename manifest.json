{
    "name": "animetick-helper",
    "version": "1.1",
    "manifest_version": 2,
    "description": "This is an anime-live life helper via Animetick, Tweetdeck, clipboard. ",
    "icons": {
        "16": "img/icon_16.png",
        "48": "img/icon_48.png",
        "128": "img/icon_128.png"
    },
    "browser_action": {
      "default_icon": "img/icon_16.png",
      "default_title": "animetick-helper",
      "default_popup": "popup.html"
    },
    "permissions": [
      "http://animetick.net/",
      "clipboardRead"
    ],
    "background": {
      "scripts": ["js/background.js"],
      "persistent": false
    }
}